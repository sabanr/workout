@inject IStringLocalizer<AppResources> L

<MudNavMenu>
    <MudNavLink Href="/" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home">
        @L["Dashboard"]
    </MudNavLink>
    <MudNavLink Href="/session" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.FitnessCenter">
        @L["ActiveSession"]
    </MudNavLink>
    <MudNavLink Href="/routines" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.ListAlt">
        @L["Routines"]
    </MudNavLink>
    <MudNavLink Href="/history" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.History">
        @L["History"]
    </MudNavLink>
</MudNavMenu>
